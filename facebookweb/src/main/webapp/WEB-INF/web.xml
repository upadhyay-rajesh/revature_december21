<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CreateProfileServlet</servlet-name>
  	<display-name>CreateProfileServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.CreateProfileServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginProfileServlet</servlet-name>
  	<display-name>LoginProfileServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.LoginProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreateProfileServlet</servlet-name>
  	<url-pattern>/CreateProfileServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginProfileServlet</servlet-name>
  	<url-pattern>/LoginProfileServlet</url-pattern>
  </servlet-mapping>
</web-app>
